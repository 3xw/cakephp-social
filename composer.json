{
  "name": "3xw/cakephp-social",
  "description": "CakePHP social parser",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.6",
    "cakephp/cakephp": "^3.4",
    "facebook/graph-sdk": "^5.6",
    "j7mbo/twitter-api-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Social\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Social\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
